{
   "$schema": "https://schemas.wp.org/trunk/block.json",
   "apiVersion": 3,
   "name": "aeawa-blocks/tabs-wrapper",
   "version": "0.1.0",
   "title": "Tabs",
   "category": "widgets",
   "icon": "media-interactive",
   "description": "Wrapper for tabs and their panels.",
   "allowedBlocks": ["aeawa-blocks/tabs-list", "aeawa-blocks/panels-list"],
   "example": {},
   "supports": {
      "interactivity": true,
      "layout": true,
      "align": true,
      "spacing": {
         "margin": true,
         "padding": true,
         "blockGap": true
      }
   },
   "attributes": {
      "activeId": {
         "type": "number",
         "default": 0
      },
      "lock": {
         "type": "boolean",
         "remove": true,
         "move": true
      }
   },
   "textdomain": "aeawa",
   "editorScript": "file:./index.js",
   "editorStyle": "file:./index.css",
   "style": "file:./style-index.css",
   "render": "file:./render.php",
   "viewScriptModule": "file:./view.js"
}
