{
   "$schema": "https://schemas.wp.org/trunk/block.json",
   "apiVersion": 3,
   "name": "artedwa-blocks/tab",
   "version": "0.1.0",
   "title": "Tab",
   "category": "widgets",
   "icon": "media-interactive",
   "description": "Tab Block",
   "example": {},
   "attributes": {
      "id": {
         "type": "number"
      },
      "isActive": {
         "type": "boolean",
         "default": false
      },
      "selectedCategory": {
         "type": "string"
      },
      "title": {
         "type": "string",
         "default": "New Tab"
      },
      "icon": {
         "type": "string"
      },
      "lock": {
         "type": "object",
         "default": {
            "remove": true,
            "move": true
         }
      }
   },
   "supports": {
      "interactivity": true
   },
   "textdomain": "artedwa",
   "editorScript": "file:./index.js",
   "editorStyle": "file:./index.css",
   "style": "file:./style-index.css",
   "render": "file:./render.php"
}
