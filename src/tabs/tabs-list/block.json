{
   "$schema": "https://schemas.wp.org/trunk/block.json",
   "apiVersion": 3,
   "name": "artedwa-blocks/tabs-list",
   "version": "0.1.0",
   "title": "Tabs List",
   "category": "widgets",
   "icon": "media-interactive",
   "description": "Contains the Tab blocks.",
   "example": {},
   "allowedBlocks": ["artedwa-blocks/tab"],
   "supports": {
      "interactivity": true,
      "layout": true,
      "align": true,
      "spacing": {
         "margin": true,
         "padding": true,
         "blockGap": true
      }
   },
   "attributes": {
      "lock": {
         "type": "object",
         "default": {
            "remove": true,
            "move": true
         }
      }
   },
   "textdomain": "artedwa",
   "editorScript": "file:./index.js",
   "editorStyle": "file:./index.css",
   "style": "file:./style-index.css",
   "render": "file:./render.php"
}
