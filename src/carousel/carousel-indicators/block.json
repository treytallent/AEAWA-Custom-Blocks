{
   "$schema": "https://schemas.wp.org/trunk/block.json",
   "apiVersion": 3,
   "name": "aeawa-blocks/carousel-indicators",
   "version": "0.1.0",
   "title": "Carousel Indicators",
   "category": "widgets",
   "icon": "media-interactive",
   "description": "An interactive block with the Interactivity API.",
   "example": {},
   "supports": {
      "interactivity": true
   },
   "attributes": {
      "id": {
         "type": "number"
      },
      "isActive": {
         "type": "boolean",
         "default": false
      },
      "indicators": {
         "type": "array",
         "default": [
            { "id": 1, "isActive": false },
            { "id": 2, "isActive": false },
            { "id": 3, "isActive": false },
            { "id": 4, "isActive": false },
            { "id": 5, "isActive": false }
         ]
      }
   },
   "textdomain": "aeawa",
   "editorScript": "file:./index.js",
   "render": "file:./render.php"
}
